<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/jiemian"
    tools:context="com.example.administrator.children_game.game3_5">

    <ImageView
        android:id="@+id/imageView_3_5_1"
        android:layout_width="286dp"
        android:layout_height="351dp"
        android:layout_marginBottom="76dp"
        android:layout_marginLeft="8dp"
        android:layout_marginRight="69dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:srcCompat="@drawable/game3_5_2" />

    <ImageView
        android:id="@+id/imageView_3_5_2"
        android:visibility="invisible"
        android:layout_width="229dp"
        android:layout_height="351dp"
        app:srcCompat="@drawable/game3_5_1"
        app:layout_constraintTop_toTopOf="@+id/imageview_3_5_true"
        android:layout_marginTop="0dp"
        android:layout_marginLeft="92dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageview_3_5_true" />

    <ImageView
        android:id="@+id/imageview_3_5_true"
        android:visibility="invisible"
        android:layout_width="212dp"
        android:layout_height="371dp"
        android:layout_marginLeft="36dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageView_3_5_1"
        app:srcCompat="@drawable/game3_5_3"
        app:layout_constraintTop_toTopOf="@+id/imageView_3_5_1"
        android:layout_marginTop="0dp" />

    <ImageView
        android:id="@+id/imageView_3_5_btn"
        android:layout_width="207dp"
        android:layout_height="84dp"
        app:srcCompat="@drawable/nothing"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/imageview_3_5_true"
        app:layout_constraintTop_toTopOf="@+id/imageview_3_5_true"
        android:layout_marginTop="-16dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="298dp"
        android:layout_height="73dp"
        android:text="混合试管中两种颜色液体"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />
</android.support.constraint.ConstraintLayout>
